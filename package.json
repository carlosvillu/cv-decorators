{
  "name": "cv-decorators",
  "version": "0.0.1",
  "description": "List of ES6 decorators",
  "main": "lib/",
  "scripts": {
    "clean:lib": "rm -rf ./lib/*",
    "predist": "npm run clean:lib -s",
    "dist": "npm run dist:scripts",
    "dist:scripts": "babel src --out-dir lib",
    "dev": "npm run dev:open && npm run dev:serve",
    "dev:serve": "webpack-dev-server --devtool eval --progress --colors --content-base docs/",
    "dev:open": "opener http://localhost:8080",
    "test": "npm run test:node",
    "test:node": "mocha --compilers js:babel/register --recursive",
    "test:node:watch": "npm run test:node -- -w",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint --ext=\".js\" src/",
    "predoc": "webpack --config webpack.doc.config.js",
    "doc": "npm run doc:build",
    "doc:build": "gh-pages -d docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/carlosvillu/cv-decorators.git"
  },
  "keywords": [
    "es6",
    "list",
    "decorators"
  ],
  "pre-commit": [
    "lint",
    "test"
  ],
  "author": "Carlos Villuendas <carlosvillu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carlosvillu/cv-decorators/issues"
  },
  "homepage": "https://github.com/carlosvillu/cv-decorators#readme",
  "devDependencies": {
    "@schibstedspain/frontend-pre-commit-rules": "latest",
    "babel": "6.5.2",
    "babel-eslint": "5.0.0",
    "babel-loader": "^6.2.4",
    "chai": "3.5.0",
    "chai-as-promised": "5.2.0",
    "css-loader": "0.23.1",
    "esdoc": "0.4.5",
    "eslint": "2.2.0",
    "eslint-plugin-react": "4.1.0",
    "gh-pages": "0.11.0",
    "mocha": "2.4.5",
    "opener": "1.4.1",
    "react-hot-loader": "1.3.0",
    "sinon": "1.17.3",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  },
  "dependencies": {
    "rx-lite": "^4.0.7"
  }
}
